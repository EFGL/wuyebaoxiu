<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gz.repair">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyAppcation"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".ProjectActivity"
            android:label="@string/title_activity_project" />
        <activity
            android:name=".ReceiveActivity"
            android:label="接单" />
        <activity
            android:name=".BackActivity"
            android:label="反馈" />
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail" />
        <activity
            android:name=".ReceiverDetailActivity"
            android:label="@string/title_activity_receiver_detail" />
        <activity
            android:name=".FeedDetailActivity"
            android:label="@string/title_activity_feed_detail"></activity>
    </application>

</manifest>
